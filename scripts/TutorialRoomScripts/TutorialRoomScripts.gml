function BinaryBackground(_value = 0, _alpha = 0, _alphaDelta = 0) constructor
{	
	value = _value;
	alpha = _alpha;
	alphaDelta = _alphaDelta;
}

function Binary3DBackground(_x, _y, _z, _value = 0) constructor
{
	x = _x
	y = _y
	z = _z
	value = _value
}